rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /public/{allPaths=**} { allow read: if true; }
    match /protected/{allPaths=**} { allow read, write: if request.auth != null; }
    match /resumes/{allPaths=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }
  }
}
